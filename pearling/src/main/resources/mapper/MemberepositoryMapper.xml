<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pearling.web.repository.MemberRepository">
	<select id="findAll" resultType="com.pearling.web.entity.Member">
      select *
      from member
   	</select>
	<select id="findById" resultType="Member">
		select * from member where id=#{id}
	</select>
	<select id="findByEamil" resultType="Member">
		select * from member where email=#{email}
	</select>
</mapper>